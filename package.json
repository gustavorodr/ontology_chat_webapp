{
  "name": "amora-organizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "./start_dev.sh",
    "dev:e2e": "./start_dev.sh && next dev -p 3010",
    "test:selenium": "jest --runInBand --testTimeout=30000 ./tests/selenium --colors",
    "test:selenium:group": "jest --runInBand --testTimeout=30000 ./tests/selenium/group.test.js --colors",
    "test:selenium:group:edit": "jest --runInBand --testTimeout=30000 ./tests/selenium/group-edit.test.js --colors",
    "test:selenium:group:create": "jest --runInBand --testTimeout=30000 ./tests/selenium/create-group.test.js --colors",
    "test:selenium:group:all": "jest --runInBand --testTimeout=30000 ./tests/selenium/group.test.js ./tests/selenium/group-edit.test.js ./tests/selenium/create-group.test.js --colors",
    "test:selenium:listing": "jest --runInBand --testTimeout=30000 ./tests/selenium/listing.test.js --colors",
    "e2e:group": "bash ./run-tests-selenium.sh group",
    "e2e:group:ui": "bash ./run-tests-selenium.sh group-ui",
    "e2e:group:edit": "bash ./run-tests-selenium.sh group-edit",
    "e2e:group:create": "bash ./run-tests-selenium.sh group-create",
    "e2e:listing": "bash ./run-tests-selenium.sh listing",
    "test:selenium:full": "./run-tests-selenium.sh",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@chatscope/chat-ui-kit-react": "^2.1.1",
    "@chatscope/chat-ui-kit-styles": "^1.4.0",
    "@mantine/core": "^8.3.4",
    "@mantine/dates": "^8.3.4",
    "@mantine/hooks": "^8.3.4",
    "@mantine/modals": "^8.3.4",
    "@mantine/notifications": "^8.3.4",
    "@tabler/icons-react": "^3.35.0",
    "autoprefixer": "^10.4.23",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "leaflet": "^1.9.4",
    "next": "15.5.4",
    "posthog-js": "^1.275.1",
    "posthog-node": "^5.9.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-leaflet": "^5.0.0",
    "sass": "^1.93.3",
    "zod": "^4.1.12"
  },
