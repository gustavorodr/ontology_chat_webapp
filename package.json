    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chromedriver": "^142.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jest": "^29.6.0",
    "lightningcss": "^1.30.2",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "selenium-webdriver": "^4.13.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
